{
  "name": "@rhangai/eslint-config-vue-typescript",
  "version": "0.7.0",
  "description": "Eslint config for vue-typescript projects",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "rhangai <hangai.renan@gmail.com>",
  "homepage": "https://github.com/rhangai/node-web-packages/tree/main/packages/eslint-config-vue-typescript#readme",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@rhangai/eslint-config-typescript": "^1.0.2",
    "eslint-plugin-vue": "^9.25.0",
    "eslint-plugin-vue-scoped-css": "^2.8.0",
    "vue-eslint-parser": "^9.4.2",
    "vue-eslint-parser-template-tokenizer-pug": "^0.4.11"
  },
  "devDependencies": {
    "eslint": "*",
    "tsup": "*"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn build:ts && yarn build:types",
    "build:ts": "tsup-node --format esm,cjs",
    "build:types": "tsup-node --dts-only",
    "typecheck": "tsc --noEmit"
  }
}
